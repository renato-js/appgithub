    <name>Minha Loja</name>
    <description>
       Loja Virtual Online
    </description>
    <author email="contato@onwebstudio.com.br" href="http://onwebstudio.com.br">
        Loja Virtual Online
    </author>
    <content src="index.html" />
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="^2.0.6" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
